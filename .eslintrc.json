{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "babelOptions": {
      "configFile": "./.babelrc"
    }
  },
  "rules": {
    "no-unused-vars": "warn"
  }
}
